$0
|> u3 {2 * arccos(sqrt(1 / 3)), 0, 0}
|> lambda x -> ctrl x [
    $0 -> (x, $0);
    $1 -> (x, $Plus)
]
|> lambda x -> ctrl x [
    ($0, $0) -> (x, $Nothing {Bit});
    ($1, y) -> (x, @Just {Bit} (y))
]
|> lambda (
    ctrl x' [
        $Nothing {Bit} -> (($0, $0), x');
        @Just {Bit} (y) -> (($1, y), x')
    ]
) -> x'
