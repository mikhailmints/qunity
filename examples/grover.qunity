def #N := 5 end
def $Answer : Num{#N} := ($0, ($1, ($1, ($0, ($0, ()))))) end

/*
Quantum oracle representing a function acting on N bits that outputs
1 for the input Answer and 0 for other inputs
*/
def @F : Num{#N} -> Bit :=
    lambda x -> match x [
        $Answer -> $1;
        else -> $0
    ]
end

$Grover{Num{#N}, $NTensorE{#N, Bit, $Plus}, @F, 4}
